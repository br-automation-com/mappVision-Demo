    <EventBinding id="EventBinding_13">
      <Source xsi:type="opcUa.Event" refId="::Vi_main:this.brdkViBase_imgTrigger_0.useCamColorForLight" event="ValueChanged" />
      <EventHandler condition="newValue = 1">
        <Sequence>
          <Step order="0">
            <Action>
              <Target xsi:type="widgets.brease.DropDownBox.Action" contentRefId="contentLights" widgetRefId="DropDownBoxFlashColor">
                <Method xsi:type="widgets.brease.DropDownBox.Action.SetVisible" value="false" />
              </Target>
            </Action>
          </Step>
          <Step order="1">
            <Action>
              <Target xsi:type="widgets.brease.Label.Action" contentRefId="contentLights" widgetRefId="labLightsColor">
                <Method xsi:type="widgets.brease.Label.Action.SetVisible" value="false" />
              </Target>
            </Action>
          </Step>
        </Sequence>
      </EventHandler>
      <EventHandler condition="newValue = 0">
        <Sequence>
          <Step order="0">
            <Action>
              <Target xsi:type="widgets.brease.DropDownBox.Action" contentRefId="contentLights" widgetRefId="DropDownBoxFlashColor">
                <Method xsi:type="widgets.brease.DropDownBox.Action.SetVisible" value="true" />
